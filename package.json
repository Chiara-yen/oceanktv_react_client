  "description": "Karaoke Station with QTS",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rm -f dist/*",
    "deploy": "npm run clean && npm run build && eslint dist/bundle.*.js",
    "build": "npm t && NODE_ENV=production webpack --config webpack.config.prod.js --progress && cp -r ./public/scripts ./dist",
    "start": "node devServer.js",
    "prepublish": "npm run deploy",
    "test": "cd ./app && NODE_ENV=test API_HOST=http://192.168.0.220 mocha --recursive --compilers js:babel-core/register",
    "cover": "cd ./app && NODE_ENV=test API_HOST=http://192.168.0.220 babel-node ../node_modules/.bin/istanbul cover --report text --report html ../node_modules/.bin/_mocha -- --recursive --compilers js:babel-core/register"
  },
