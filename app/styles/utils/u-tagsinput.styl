.react-tagsinput {
  background-color #fff
  border 1px solid #ccc
  overflow hidden
  padding-left 5px
  padding-top 5px
}

.react-tagsinput-tag {
  display inline-flex
  align-items center
  background-color #fff
  color #000
  font-family sans-serif
  font-size 13px
  font-weight 400
  padding 5px
  border 1px solid #aaa
  border-radius 2px
  margin-bottom 5px
  margin-right 5px
}

.react-tagsinput-remove {
  display inline-flex
  justify-content center
  align-items center
  background-color #aaa
  font-size 7px
  line-height 1.2
  width 20px
  height 20px
  border-radius 50%
  margin-left 5px
  cursor pointer
}

.react-tagsinput-input {
  background transparent
  border 0
  color #777
  font-family sans-serif
  font-size 13px
  font-weight 400
  line-height 2
  margin-bottom 6px
  margin-top 1px
  outline none
  padding 5px
}
